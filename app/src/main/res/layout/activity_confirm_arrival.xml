<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.wareproz.mac.gravydriver.ConfirmArrivalActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:background="@color/colorPrimaryDark"
        android:id="@+id/relativeLayout9"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Gravy Driver"
            android:id="@+id/textView17"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true"
            android:textColor="@color/colorGrey" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="75dp"
        android:id="@+id/relativeLayout12"
        android:background="#FFFFFF"
        android:layout_below="@+id/relativeLayout9"
        android:layout_centerHorizontal="true">

        <RelativeLayout
            android:layout_width="2dp"
            android:layout_height="match_parent"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:layout_toLeftOf="@+id/relativeLayout11"
            android:layout_toStartOf="@+id/relativeLayout11"
            android:background="@drawable/rectangle_background"
            android:id="@+id/relativeLayout10" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="11 Majekodunmi Street, Allen Avenue, Ikeja Lagos"
            android:id="@+id/textView18"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_toLeftOf="@+id/relativeLayout10"
            android:layout_toStartOf="@+id/relativeLayout10"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="true"
            android:layout_margin="10dp"
            android:typeface="serif" />

        <RelativeLayout
            android:layout_width="90dp"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:background="#FFFFFF"
            android:layout_alignParentEnd="true"
            android:id="@+id/relativeLayout11" >

            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:id="@+id/imageView8"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"
                android:src="@drawable/gravy_logo" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="NAVIGATE"
                android:id="@+id/textView19"
                android:layout_below="@+id/imageView8"
                android:layout_centerHorizontal="true"
                android:textSize="12dp"
                android:typeface="normal"
                android:textStyle="bold"
                android:layout_marginTop="6dp" />
        </RelativeLayout>
    </RelativeLayout>

    <fragment
        android:id="@+id/fragment3"
        android:name="com.google.android.gms.maps.MapFragment"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_below="@+id/relativeLayout12"
        android:layout_alignLeft="@+id/relativeLayout12"
        android:layout_alignStart="@+id/relativeLayout12"
        android:layout_above="@+id/relativeLayout13" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:id="@+id/relativeLayout13">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:background="#FFFFFF"
            android:id="@+id/relativeLayout14"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true">

            <ImageView
                android:id="@+id/imageView9"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:src="@drawable/profilepic"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginLeft="10dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="1 mins"
                android:id="@+id/textView20"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:layout_marginRight="10dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Pick up"
                android:id="@+id/textView21"
                android:layout_alignTop="@+id/imageView9"
                android:layout_toRightOf="@+id/imageView9"
                android:layout_toEndOf="@+id/imageView9"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="2dp"
                android:textColor="#7DC24F"
                android:textStyle="bold" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Chumylee"
                android:id="@+id/textView22"
                android:layout_alignTop="@+id/textView20"
                android:layout_toRightOf="@+id/imageView9"
                android:layout_toEndOf="@+id/imageView9"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="7dp" />

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:id="@+id/imageView10"
                android:layout_alignParentBottom="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="1dp"
                android:src="@drawable/gravy_logo" />
        </RelativeLayout>

        <Button
            style="?android:textAppearanceSmall"
            android:id="@+id/button2"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:text="CONFIRM ARRIVAL"
            android:textStyle="bold"
            android:textColor="#FFF"
            android:textSize="20dp"
            android:background="#005A98"
            android:layout_below="@+id/relativeLayout14"
            android:layout_centerHorizontal="true"
            android:layout_margin="15dp" />

    </RelativeLayout>
</RelativeLayout>
